{
  "flows": [
    {
      "id": "subflow_call",
      "initial": "start",
      "blocks": {
        "start": {
          "type": "TPlay",
          "message": "Welcome to parent flow",
          "next": "survey"
        },
        "survey": {
          "type": "TCall",
          "collection": "simple_survey",
          "callError": "error",
          "message": "This is subflow. How much do you like in to cloud DSL?",
          "success": "finish",
          "error": "error"
        },
        "finish": {
          "type": "TPlay",
          "message": "This is parent flow. Thank you. Bye-bye",
          "next": "disconnect"
        },
        "error": {
          "type": "TPlay",
          "message": "Oh no. Unexpected error. Please call later",
          "next": "disconnect"
        },
        "disconnect": {
          "type": "TDisconnect"
        }
      }
    }
  ]
}
